{
  "manifest_version": 3,
  "name": "React Movie Detector Chrome Extension",
  "version": "1.0.0",
  "description": "A basic example extension with only required keys",
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation",
    "scripting",
    "storage"
    
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://www.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.google.com/search*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
   
  }
}